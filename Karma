-- ‚ñà‚ñÄ‚ñÄ ‚ñà‚ñÄ‚ñÄ‚ñà ‚ñà‚ñÄ‚ñÄ‚ñÑ ‚ñà‚ñÄ‚ñÄ ‚ñÄ‚ñÄ‚ñà‚ñÄ‚ñÄ ‚ñà‚ñÄ‚ñÄ ‚ñà‚ñÄ‚ñÄ‚ñà ‚ñà‚ñÄ‚ñÄ‚ñÑ 
-- ‚ñà‚ñë‚ñë ‚ñà‚ñÑ‚ñÑ‚ñà ‚ñà‚ñÄ‚ñÄ‚ñÑ ‚ñà‚ñÄ‚ñÄ ‚ñë‚ñë‚ñà‚ñë‚ñë ‚ñà‚ñÄ‚ñÄ ‚ñà‚ñÑ‚ñÑ‚ñà ‚ñà‚ñÄ‚ñÄ‚ñÑ 
-- ‚ñÄ‚ñÄ‚ñÄ ‚ñÄ‚ñë‚ñë‚ñÄ ‚ñÄ‚ñÄ‚ñÄ‚ñë ‚ñÄ‚ñÄ‚ñÄ ‚ñë‚ñë‚ñÄ‚ñë‚ñë ‚ñÄ‚ñÄ‚ñÄ ‚ñÄ‚ñë‚ñë‚ñÄ ‚ñÄ‚ñÄ‚ñÄ‚ñë 
-- –ê–í–¢–û–†–°–ö–ê–Ø –†–ê–ó–†–ê–ë–û–¢–ö–ê - –ö–û–î 100% –£–ù–ò–ö–ê–õ–ï–ù

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local RunService = game:GetService("RunService")
local Workspace = game:GetService("Workspace")
local Lighting = game:GetService("Lighting")
local TweenService = game:GetService("TweenService")

-- üé® –ó–ê–ì–†–£–ó–ö–ê –ë–ò–ë–õ–ò–û–¢–ï–ö–ò –ò–ù–¢–ï–†–§–ï–ô–°–ê
local Success, Rayfield = pcall(function()
    return loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
end)

if not Success then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "–û—à–∏–±–∫–∞ –∑–∞–≥—Ä—É–∑–∫–∏",
        Text = "–ë–∏–±–ª–∏–æ—Ç–µ–∫–∞ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ –Ω–µ–¥–æ—Å—Ç—É–ø–Ω–∞",
        Duration = 5
    })
    return
end

-- üèóÔ∏è –°–û–ó–î–ê–ù–ò–ï –ì–õ–ê–í–ù–û–ì–û –û–ö–ù–ê
local Window = Rayfield:CreateWindow({
    Name = "üî• MM2 ULTIMATE SUITE v2.0",
    LoadingTitle = "–ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –∞–≤—Ç–æ—Ä—Å–∫–æ–≥–æ —Ö–∞–±–∞...",
    LoadingSubtitle = "–ó–∞–≥—Ä—É–∑–∫–∞ —É–Ω–∏–∫–∞–ª—å–Ω—ã—Ö –º–æ–¥—É–ª–µ–π",
    ConfigurationSaving = { 
        Enabled = true,
        FolderName = "MM2UltimateSuite",
        FileName = "PremiumConfig"
    },
    Discord = {
        Enabled = false
    },
    KeySystem = false
})

-- üìä –°–ò–°–¢–ï–ú–ê –ü–ï–†–ï–ú–ï–ù–ù–´–• –ò –ö–û–ù–§–ò–ì–£–†–ê–¶–ò–ò
local Config = {
    ESP = {
        Enabled = false,
        Players = {},
        MaxDistance = 500,
        ShowNames = true,
        ShowDistance = true,
        ShowRoles = true
    },
    Combat = {
        KillAura = false,
        AutoKill = false,
        ReachDistance = 25,
        SilentAim = false
    },
    Movement = {
        Fly = false,
        Speed = 16,
        JumpPower = 50,
        Noclip = false
    },
    Visuals = {
        Fullbright = false,
        Chams = false,
        WorldColor = Color3.new(1, 1, 1)
    }
}

-- üéØ –ú–û–î–£–õ–¨ 1: –£–ú–ù–´–ô ESP –° –ò–ò-–ê–ù–ê–õ–ò–ó–û–ú
local ESPModule = {
    Objects = {},
    Connections = {}
}

function ESPModule:CreatePlayerESP(player)
    if player == LocalPlayer then return end
    if self.Objects[player] then return end
    
    -- üîç –ò–ò-–æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ —Ä–æ–ª–∏ —Å –º–Ω–æ–∂–µ—Å—Ç–≤–µ–Ω–Ω–æ–π –ø—Ä–æ–≤–µ—Ä–∫–æ–π
    local function DetermineRole(targetPlayer)
        if not targetPlayer.Character then return "Innocent" end
        
        -- –ê–Ω–∞–ª–∏–∑ –∏–Ω–≤–µ–Ω—Ç–∞—Ä—è
        local backpack = targetPlayer:FindFirstChild("Backpack")
        local character = targetPlayer.Character
        
        -- –ú–Ω–æ–≥–æ—É—Ä–æ–≤–Ω–µ–≤–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç–æ–≤
        local checkPlaces = {backpack, character}
        for _, place in ipairs(checkPlaces) do
            if place then
                for _, item in ipairs(place:GetChildren()) do
                    if item:IsA("Tool") then
                        local itemName = string.lower(item.Name)
                        if string.find(itemName, "knife") or string.find(itemName, "–Ω–æ–∂") then
                            return "Murderer", Color3.fromRGB(255, 50, 50)
                        elseif string.find(itemName, "gun") or string.find(itemName, "revolver") then
                            return "Sheriff", Color3.fromRGB(80, 120, 255)
                        end
                    end
                end
            end
        end
        
        return "Innocent", Color3.fromRGB(50, 255, 50)
    end

    -- üé® –°–æ–∑–¥–∞–Ω–∏–µ –≤–∏–∑—É–∞–ª—å–Ω—ã—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤
    local role, color = DetermineRole(player)
    
    local highlight = Instance.new("Highlight")
    highlight.Name = "ESP_" .. player.Name
    highlight.Adornee = player.Character
    highlight.FillColor = color
    highlight.OutlineColor = Color3.new(1, 1, 1)
    highlight.FillTransparency = 0.6
    highlight.OutlineTransparency = 0.3
    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    
    local billboard = Instance.new("BillboardGui")
    billboard.Name = "Info_" .. player.Name
    billboard.Size = UDim2.new(0, 200, 0, 80)
    billboard.StudsOffset = Vector3.new(0, 3.5, 0)
    billboard.AlwaysOnTop = true
    billboard.Adornee = player.Character:WaitForChild("Head")
    
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Size = UDim2.new(1, 0, 0.4, 0)
    nameLabel.BackgroundTransparency = 1
    nameLabel.Text = player.Name
    nameLabel.TextColor3 = Color3.new(1, 1, 1)
    nameLabel.TextSize = 14
    nameLabel.Font = Enum.Font.GothamBold
    
    local roleLabel = Instance.new("TextLabel")
    roleLabel.Size = UDim2.new(1, 0, 0.3, 0)
    roleLabel.Position = UDim2.new(0, 0, 0.4, 0)
    roleLabel.BackgroundTransparency = 1
    roleLabel.Text = role
    roleLabel.TextColor3 = color
    roleLabel.TextSize = 12
    roleLabel.Font = Enum.Font.Gotham
    
    local distanceLabel = Instance.new("TextLabel")
    distanceLabel.Size = UDim2.new(1, 0, 0.3, 0)
    distanceLabel.Position = UDim2.new(0, 0, 0.7, 0)
    distanceLabel.BackgroundTransparency = 1
    distanceLabel.TextColor3 = Color3.new(1, 1, 1)
    distanceLabel.TextSize = 11
    distanceLabel.Font = Enum.Font.Gotham
    
    roleLabel.Parent = billboard
    nameLabel.Parent = billboard
    distanceLabel.Parent = billboard
    billboard.Parent = player.Character.Head
    highlight.Parent = player.Character
    
    self.Objects[player] = {
        Highlight = highlight,
        Billboard = billboard,
        NameLabel = nameLabel,
        RoleLabel = roleLabel,
        DistanceLabel = distanceLabel,
        Role = role,
        Color = color
    }
end

function ESPModule:UpdateESP()
    for player, data in pairs(self.Objects) do
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            -- üìè –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –¥–∏—Å—Ç–∞–Ω—Ü–∏–∏
            local distance = (LocalPlayer.Character.HumanoidRootPart.Position - 
                            player.Character.HumanoidRootPart.Position).Magnitude
            data.DistanceLabel.Text = math.floor(distance) .. " studs"
            
            -- üé® –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –≤–∏–¥–∏–º–æ—Å—Ç–∏ –Ω–∞ –æ—Å–Ω–æ–≤–µ –¥–∏—Å—Ç–∞–Ω—Ü–∏–∏
            local alpha = math.clamp(1 - (distance / Config.ESP.MaxDistance), 0.1, 1)
            data.Highlight.FillTransparency = 0.6 + (0.4 * (1 - alpha))
        else
            self:RemovePlayerESP(player)
        end
    end
end

function ESPModule:RemovePlayerESP(player)
    if self.Objects[player] then
        for _, element in pairs(self.Objects[player]) do
            if element and element.Parent then
                element:Destroy()
            end
        end
        self.Objects[player] = nil
    end
end

-- ‚öîÔ∏è –ú–û–î–£–õ–¨ 2: –ö–û–ú–ë–ê–¢-–°–ò–°–¢–ï–ú–ê –° –ò–ò-–ü–†–ò–û–†–ò–¢–ï–¢–ê–ú–ò
local CombatModule = {
    Active = false,
    Targets = {},
    PriorityList = {"Murderer", "Sheriff", "Innocent"}
}

function CombatModule:StartAutoKill()
    self.Active = true
    
    coroutine.wrap(function()
        while self.Active do
            for _, player in ipairs(Players:GetPlayers()) do
                if player ~= LocalPlayer and player.Character then
                    -- üéØ –ò–ò-–≤—ã–±–æ—Ä —Ü–µ–ª–∏ –ø–æ –ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç—É
                    local role = ESPModule.Objects[player] and ESPModule.Objects[player].Role or "Innocent"
                    local priority = table.find(self.PriorityList, role) or 3
                    
                    if priority <= 2 then -- –ê—Ç–∞–∫—É–µ–º —Ç–æ–ª—å–∫–æ —É–±–∏–π—Ü –∏ —à–µ—Ä–∏—Ñ–æ–≤
                        self:ExecuteKill(player)
                        wait(0.5) -- –ó–∞–¥–µ—Ä–∂–∫–∞ –º–µ–∂–¥—É –∞—Ç–∞–∫–∞–º–∏
                    end
                end
            end
            wait(0.1)
        end
    end)()
end

function CombatModule:ExecuteKill(target)
    if not target.Character then return end
    
    local humanoid = target.Character:FindFirstChild("Humanoid")
    local rootPart = target.Character:FindFirstChild("HumanoidRootPart")
    
    if humanoid and rootPart and humanoid.Health > 0 then
        -- üéØ –£–º–Ω–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –∏ –∞—Ç–∞–∫–∞
        local originalCFrame = LocalPlayer.Character.HumanoidRootPart.CFrame
        
        -- –ü–ª–∞–≤–Ω–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è
        local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
        local tween = TweenService:Create(
            LocalPlayer.Character.HumanoidRootPart,
            tweenInfo,
            {CFrame = rootPart.CFrame * CFrame.new(0, 0, -2)}
        )
        tween:Play()
        
        tween.Completed:Wait()
        
        -- ‚ö° –ú–≥–Ω–æ–≤–µ–Ω–Ω–∞—è –∞—Ç–∞–∫–∞
        if LocalPlayer.Character:FindFirstChildOfClass("Tool") then
            LocalPlayer.Character:FindFirstChildOfClass("Tool"):Activate()
        end
        
        -- üîÑ –í–æ–∑–≤—Ä–∞—Ç –Ω–∞ –ø–æ–∑–∏—Ü–∏—é
        local returnTween = TweenService:Create(
            LocalPlayer.Character.HumanoidRootPart,
            tweenInfo,
            {CFrame = originalCFrame}
        )
        returnTween:Play()
    end
end

-- üöÄ –ú–û–î–£–õ–¨ 3: –†–ê–°–®–ò–†–ï–ù–ù–û–ï –ü–ï–†–ï–ú–ï–©–ï–ù–ò–ï
local MovementModule = {
    Flying = false,
    OriginalGravity = nil
}

function MovementModule:ToggleFlight(state)
    self.Flying = state
    
    if state then
        self.OriginalGravity = Workspace.Gravity
        Workspace.Gravity = 0
        
        local bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.Name = "FlightVelocity"
        bodyVelocity.Velocity = Vector3.new(0, 0, 0)
        bodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
        bodyVelocity.Parent = LocalPlayer.Character.HumanoidRootPart
        
        -- üéÆ –£–º–Ω–æ–µ —É–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –ø–æ–ª–µ—Ç–æ–º
        RunService.Heartbeat:Connect(function()
            if not self.Flying then return end
            
            local camera = Workspace.CurrentCamera
            local rootPart = LocalPlayer.Character.HumanoidRootPart
            
            local direction = Vector3.new(0, 0, 0)
            local speed = 50
            
            -- WASD + Space/Shift —É–ø—Ä–∞–≤–ª–µ–Ω–∏–µ
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.W) then
                direction = direction + camera.CFrame.LookVector
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.S) then
                direction = direction - camera.CFrame.LookVector
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.A) then
                direction = direction - camera.CFrame.RightVector
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.D) then
                direction = direction + camera.CFrame.RightVector
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.Space) then
                direction = direction + Vector3.new(0, 1, 0)
            end
            if game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftShift) then
                direction = direction + Vector3.new(0, -1, 0)
            end
            
            bodyVelocity.Velocity = direction.Unit * speed
        end)
    else
        Workspace.Gravity = self.OriginalGravity
        local velocity = LocalPlayer.Character.HumanoidRootPart:FindFirstChild("FlightVelocity")
        if velocity then
            velocity:Destroy()
        end
    end
end

-- üé® –ì–õ–ê–í–ù–´–ô –ò–ù–¢–ï–†–§–ï–ô–°
local MainTab = Window:CreateTab("üè† –ì–ª–∞–≤–Ω–∞—è –ø–∞–Ω–µ–ª—å", 10734948080)

-- üìä –°–ï–ö–¶–ò–Ø –ò–ù–§–û–†–ú–ê–¶–ò–ò
local InfoSection = MainTab:CreateSection("üìä –°—Ç–∞—Ç—É—Å –∏–≥—Ä—ã")

local PlayerCountLabel = MainTab:CreateLabel("–ò–≥—Ä–æ–∫–æ–≤ –æ–Ω–ª–∞–π–Ω: " .. #Players:GetPlayers())
local RoundTimeLabel = MainTab:CreateLabel("–í—Ä–µ–º—è —Ä–∞—É–Ω–¥–∞: 0:00")
local MurdererLabel = MainTab:CreateLabel("üî™ –£–±–∏–π—Ü–∞: –ù–µ –æ–ø—Ä–µ–¥–µ–ª–µ–Ω")
local SheriffLabel = MainTab:CreateLabel("üî´ –®–µ—Ä–∏—Ñ: –ù–µ –æ–ø—Ä–µ–¥–µ–ª–µ–Ω")

-- üëÅÔ∏è –°–ï–ö–¶–ò–Ø –í–ò–ó–£–ê–õ–ò–ó–ê–¶–ò–ò
local VisualSection = MainTab:CreateSection("üëÅÔ∏è –£–º–Ω–∞—è –≤–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏—è")

local ESPToggle = MainTab:CreateToggle({
    Name = "üéØ –£–º–Ω—ã–π ESP —Å –ò–ò-–∞–Ω–∞–ª–∏–∑–æ–º",
    CurrentValue = false,
    Callback = function(Value)
        Config.ESP.Enabled = Value
        
        if Value then
            -- –ê–∫—Ç–∏–≤–∞—Ü–∏—è ESP –¥–ª—è –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤
            for _, player in ipairs(Players:GetPlayers()) do
                if player ~= LocalPlayer and player.Character then
                    ESPModule:CreatePlayerESP(player)
                end
            end
        else
            -- –î–µ–∞–∫—Ç–∏–≤–∞—Ü–∏—è ESP
            for player in pairs(ESPModule.Objects) do
                ESPModule:RemovePlayerESP(player)
            end
        end
    end
})

local ChamsToggle = MainTab:CreateToggle({
    Name = "üåà –¶–≤–µ—Ç–Ω—ã–µ —á–∞–º—Å—ã –ø–æ —Ä–æ–ª—è–º",
    CurrentValue = false,
    Callback = function(Value)
        Config.Visuals.Chams = Value
    end
})

-- ‚öîÔ∏è –°–ï–ö–¶–ò–Ø –ë–û–Ø
local CombatSection = MainTab:CreateSection("‚öîÔ∏è –£–º–Ω—ã–π –±–æ–π")

local AutoKillToggle = MainTab:CreateToggle({
    Name = "üíÄ –ò–ò-–∞–≤—Ç–æ–∫–∏–ª–ª (–ø—Ä–∏–æ—Ä–∏—Ç–µ—Ç –ø–æ —Ä–æ–ª—è–º)",
    CurrentValue = false,
    Callback = function(Value)
        if Value then
            CombatModule:StartAutoKill()
        else
            CombatModule.Active = false
        end
    end
})

local KillAllButton = MainTab:CreateButton({
    Name = "‚ö° –ú–≥–Ω–æ–≤–µ–Ω–Ω—ã–π Kill All",
    Callback = function()
        Rayfield:Notify({
            Title = "‚ö° –ê–∫—Ç–∏–≤–∞—Ü–∏—è Kill All",
            Content = "–ù–∞—á–∏–Ω–∞–µ–º —É–º–Ω–æ–µ —É–Ω–∏—á—Ç–æ–∂–µ–Ω–∏–µ...",
            Duration = 3
        })
        
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                CombatModule:ExecuteKill(player)
                wait(0.2)
            end
        end
    end
})

-- üöÄ –°–ï–ö–¶–ò–Ø –ü–ï–†–ï–ú–ï–©–ï–ù–ò–Ø
local MovementSection = MainTab:CreateSection("üöÄ –†–∞—Å—à–∏—Ä–µ–Ω–Ω–æ–µ –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏–µ")

local FlyToggle = MainTab:CreateToggle({
    Name = "üïäÔ∏è –£–º–Ω—ã–π –ø–æ–ª–µ—Ç (WASD + Space/Shift)",
    CurrentValue = false,
    Callback = function(Value)
        MovementModule:ToggleFlight(Value)
    end
})

local SpeedSlider = MainTab:CreateSlider({
    Name = "üí® –°–∫–æ—Ä–æ—Å—Ç—å –ø–µ—Ä–µ–¥–≤–∏–∂–µ–Ω–∏—è",
    Range = {16, 100},
    Increment = 5,
    Suffix = "studs/s",
    CurrentValue = 16,
    Callback = function(Value)
        LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
})

-- üîß –î–û–ü–û–õ–ù–ò–¢–ï–õ–¨–ù–´–ï –§–£–ù–ö–¶–ò–ò
local UtilityTab = Window:CreateTab("üîß –£—Ç–∏–ª–∏—Ç—ã", 10734948080)

local TeleportSection = UtilityTab:CreateSection("üåÄ –£–º–Ω–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è")

UtilityTab:CreateButton({
    Name = "üî´ –ê–≤—Ç–æ–ø–æ–¥–±–æ—Ä –ø—É—à–∫–∏",
    Callback = function()
        local gun = Workspace:FindFirstChild("GunDrop")
        if gun then
            LocalPlayer.Character.HumanoidRootPart.CFrame = gun.CFrame
        end
    end
})

UtilityTab:CreateButton({
    Name = "üéØ –¢–µ–ª–µ–ø–æ—Ä—Ç –∫ —É–±–∏–π—Ü–µ",
    Callback = function()
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and ESPModule.Objects[player] then
                if ESPModule.Objects[player].Role == "Murderer" then
                    LocalPlayer.Character.HumanoidRootPart.CFrame = 
                        player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -3)
                    break
                end
            end
        end
    end
})

-- ‚öôÔ∏è –°–ò–°–¢–ï–ú–ê –û–ë–ù–û–í–õ–ï–ù–ò–ô
local UpdateTab = Window:CreateTab("üîÑ –°–∏—Å—Ç–µ–º–∞", 10734948080)

UpdateTab:CreateButton({
    Name = "üßπ –û—á–∏—Å—Ç–∏—Ç—å –≤—Å–µ —ç—Ñ—Ñ–µ–∫—Ç—ã",
    Callback = function()
        for player in pairs(ESPModule.Objects) do
            ESPModule:RemovePlayerESP(player)
        end
        MovementModule:ToggleFlight(false)
        CombatModule.Active = false
    end
})

-- üîÑ –û–°–ù–û–í–ù–û–ô –¶–ò–ö–õ –û–ë–ù–û–í–õ–ï–ù–ò–Ø
RunService.Heartbeat:Connect(function()
    -- üìä –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏
    PlayerCountLabel:Set("–ò–≥—Ä–æ–∫–æ–≤ –æ–Ω–ª–∞–π–Ω: " .. #Players:GetPlayers())
    
    -- üëÅÔ∏è –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ ESP
    if Config.ESP.Enabled then
        ESPModule:UpdateESP()
        
        -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –¥–æ–±–∞–≤–ª–µ–Ω–∏–µ –Ω–æ–≤—ã—Ö –∏–≥—Ä–æ–∫–æ–≤
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character and not ESPModule.Objects[player] then
                ESPModule:CreatePlayerESP(player)
            end
        end
    end
end)

-- üéâ –§–ò–ù–ê–õ–¨–ù–ê–Ø –ê–ö–¢–ò–í–ê–¶–ò–Ø
Rayfield:Notify({
    Title = "üéÆ MM2 ULTIMATE SUITE v2.0",
    Content = "–£–Ω–∏–∫–∞–ª—å–Ω—ã–π –∞–≤—Ç–æ—Ä—Å–∫–∏–π —á–∏—Ç –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω!\n–í—Å–µ –º–æ–¥—É–ª–∏ –∑–∞–≥—Ä—É–∂–µ–Ω—ã —É—Å–ø–µ—à–Ω–æ.",
    Duration = 8,
})

print([[
    
 ‚ñà‚ñÄ‚ñÄ ‚ñà‚ñÄ‚ñÄ‚ñà ‚ñà‚ñÄ‚ñÄ‚ñÑ ‚ñà‚ñÄ‚ñÄ ‚ñÄ‚ñÄ‚ñà‚ñÄ‚ñÄ ‚ñà‚ñÄ‚ñÄ ‚ñà‚ñÄ‚ñÄ‚ñà ‚ñà‚ñÄ‚ñÄ‚ñÑ 
