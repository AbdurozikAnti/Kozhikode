--=== Ð£ÐÐ˜ÐšÐÐ›Ð¬ÐÐ«Ð• ÐŸÐ•Ð Ð•ÐœÐ•ÐÐÐ«Ð• ===--
local UniqueESP = {
    Enabled = true,
    RainbowMode = false,
    PulseMode = false,
    GlowIntensity = 1
}

local Combat = {
    GhostKill = false, -- Ð£Ð±Ð¸Ð¹ÑÑ‚Ð²Ð¾ Ñ‡ÐµÑ€ÐµÐ· ÑÑ‚ÐµÐ½Ñ‹
    TimeStop = false, -- Ð—Ð°Ð¼ÐµÐ´Ð»ÐµÐ½Ð¸Ðµ Ð²Ñ€ÐµÐ¼ÐµÐ½Ð¸ Ð´Ð»Ñ Ð²Ñ€Ð°Ð³Ð¾Ð²
